programa {
  funcao inicio() {
    //DEclaracao
    real V1,V2,V3, media_aritmetica
    escreva ("informe o valor gasto no primeiro mes: ")
    leia (V1)
    escreva ("informe o valor gasto no segundo mes: ")
    leia (V2)
    escreva ("informe o valor gasto no terceiro mes: ")
    leia (V3)
    //Processamento
    media_aritmetica = (V1+V2+V3)/3
    //Saida de dados
    escreva ("esta e a media aritmetica:",media_aritmetica)
  }
}
