programa {
  funcao inicio() {
  //declaracao de variaveis
  real VX, VY, troca 
  //entrada de dados
  escreva ("informe o valor x: ")
  leia (VX)
  escreva ("informe o valor de y: ")
  leia (VY)
  //process
  troca = VX 
  VX = VY
  VY = troca
  //saida
  escreva ("esse é o novo valor de x é: ",VX," e esse é o de y: ",VY," ")
  }
}
